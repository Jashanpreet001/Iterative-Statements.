#include <stdio.h>

int main(){
    
    int num,i;
    printf("enter the number you want the number table of\n");
    scanf("%d",&num);
    
    for(i=1;i<=10;i++)
    {  printf("%d * %d = %d\n",num,i,num*i);
    }
    
    return 0;
    
}
